<% include partials/header %>


<% blogs.forEach(function(blog) { %>
<div class="main container">
    <h1>
        <%= blog.title %>
    </h1>
    <img src="<%= blog.image %>" alt="">
    <span><%= blog.date %></span>
    <p>
        <%= blog.body %>
    </p>
</div>
<%  })%>

<% include partials/footer %>